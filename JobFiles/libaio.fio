[global]
time_based=1
runtime=30
clocksource=gettimeofday
ramp_time=10
startdelay=20
ioengine=libaio
direct=1
bs=4k
iodepth=8
rw=randrw
rwmixread=80

[job1]
filename=/dev/rbd0

[job2]
filename=/dev/rbd1

[job3]
filename=/dev/rbd2

[job4]
filename=/dev/rbd3

[job5]
filename=/dev/rbd4

[job6]
filename=/dev/rbd5

[job7]
filename=/dev/rbd6

[job8]
filename=/dev/rbd7


